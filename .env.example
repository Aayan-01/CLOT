# Example environment variables (copy this file to .env and fill in values)
## Google API / Gemini keys (prefer Secret Manager in production)
GOOGLE_CLOUD_GEMINI_API_KEY=your_gemini_pro_vision_api_key_here
GOOGLE_AI_STUDIO_FLASH_API_KEY=your_gemini_flash_api_key_here
SESSION_SECRET=your_random_session_secret_here
PORT=8080
ALLOWED_ORIGINS=http://localhost:5173
# Optional: Storage, DB, or third-party settings
GCS_BUCKET_NAME=retro_rate
GCS_MAKE_PUBLIC=true
FIRESTORE_PROJECT=your-firestore-project-id
REDIS_URL=redis://localhost:6379
NODE_ENV=development

# For local dev the client will point to the backend running on localhost:4000
# In production set VITE_API_ORIGIN to the backend URL (for example the Cloud Run service URL)
VITE_API_ORIGIN=http://localhost:4000

# Optional social links (if you want to display them in the client)
# The client is Vite-based so for the client set these in client/.env or CI
VITE_SOCIAL_TWITTER=
VITE_SOCIAL_INSTAGRAM=
VITE_SOCIAL_LINKEDIN=
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
